# Copy this file to .env and fill in your values. Do not commit .env.

# Google OAuth (backend and frontend)
GOOGLE_CLIENT_ID=
VITE_GOOGLE_CLIENT_ID=

# PayPal (subscription; backend and frontend)
PAYPAL_CLIENT_ID=
PAYPAL_CLIENT_SECRET=
PAYPAL_MODE=sandbox
PAYPAL_PLAN_SILVER=
PAYPAL_PLAN_GOLD=
PAYPAL_RETURN_URL=http://localhost/subscription-success
PAYPAL_CANCEL_URL=http://localhost/
VITE_PAYPAL_CLIENT_ID=

# Frontend API URL (e.g. http://localhost for local, or your deployed backend URL)
VITE_API_URL=

# Optional: for production (e.g. Render) use DATABASE_URL instead of individual DB vars
# DATABASE_URL=postgresql://user:password@host:5432/database
